#effect
	width 100px
	height 100px
	border 1px solid black

.highlight
	background-color red
	width 200px !important

.shrink
	background-color gray
	width 50px !important

.square
	width 100px
	height 100px

.border
	border 1px solid red

.colored
	background-color purple

.progress-bar
	width 100%
	height 20px
	background-color gray
	overflow hidden

.bar
	height 20px
	background-color red
	width 10px
	transition width .5s ease;
